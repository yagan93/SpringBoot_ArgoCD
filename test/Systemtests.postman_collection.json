	"item": [
		{
			"name": "Get Random Joke",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Test for HTTP status code\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// Test for Content-Type\r",
							"pm.test(\"Content-Type is application/json\", function () {\r",
							"    pm.expect(pm.response.headers.get('Content-Type')).to.include('application/json');\r",
							"});\r",
							"\r",
							"// Test if response has required keys\r",
							"pm.test(\"Response has all required keys\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.have.keys('id', 'type', 'setup', 'punchline');\r",
							"});\r",
							"\r",
							"// Test for non-empty joke setup\r",
							"pm.test(\"Joke setup is not empty\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.setup).to.not.be.empty;\r",
							"});\r",
							"\r",
							"// Test for non-empty punchline\r",
							"pm.test(\"Punchline is not empty\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.punchline).to.not.be.empty;\r",
							"});\r",
							"\r",
							"// Test if joke type is 'general'\r",
							"pm.test(\"Joke type is 'general'\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.type).to.eql('general');\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
